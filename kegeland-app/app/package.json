{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "prepare": "cd .. && husky install ./app/.husky",
    "test:unit": "jest",
    "start:e2e": "cross-env RN_SRC_EXT=e2e.ts,e2e.js NODE_ENV=test react-native start",
    "start:integration": "cross-env NODE_ENV=integration react-native start",
    "e2e:build-android-local": "detox build -c android.emu.local",
    "e2e:test-android-local": "detox test -c android.emu.local",
    "e2e:local": "detox build -c android.emu.local & detox test -c android.emu.local",
    "test:e2e": "concurrently \"yarn start:e2e\" \"yarn e2e:local\"",
    "test:integration": "concurrently \"yarn start:integration\" \"yarn e2e:local\""
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.8",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "detox": "18.20.2",
    "jest-circus": "^26.6.3",
    "lodash": "^4.17.21",
    "matter-js": "^0.18.0",
    "react": "17.0.2",
    "react-hook-form": "^7.36.1",
    "react-native": "0.66.4",
    "react-native-ble-manager": "^8.4.3",
    "react-native-cli": "^2.0.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-game-engine": "^1.2.0",
    "react-native-gesture-handler": "^2.6.2",
    "react-native-paper": "^4.12.5",
    "react-native-reanimated": "2.8.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-system-navigation-bar": "^2.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.2.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.1.0",
    "@testing-library/jest-native": "^4.0.12",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.1.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/axios": "^0.14.0",
    "@types/detox": "^18.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.185",
    "@types/matter-js": "^0.18.2",
    "@types/node": "^18.11.2",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.1",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.7.1",
    "react-native-flipper": "^0.164.0",
    "react-test-renderer": "17.0.2",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga-test-plan": "^4.0.6",
    "typescript": "^4.8.3"
  }
